
<h4 class="text-info">Professional Profile</h4>
<form>
  <div class="form-group">
    <lable for="degree">Degree</lable>
    <input class="form-control" type="text" readonly id="degree" value='<%=doctor.degree%>' >
  </div>
  <div class="form-group">
    <label for="category">Category</label>
    <input class="form-control" type="text" readonly id="category" value='<%=doctor.category%>' >
  </div>
  <div class="form-group">
    <lable for="speciality">Speciality</lable>
    <input class="form-control" type="text" readonly id="speciality" value="<%=doctor.speciality%>" >
  </div>
  <div class="form-group">
    <lable for="experience">Experience</lable>
    <input class="form-control" type="text" readonly id="experience" value="<%=doctor.experience%>" >
  </div>
  <div class="form-group">
    <lable for="fee">Consultation fee</lable>
    <input class="form-control" type="text" readonly id="fee" value="<%=doctor.fee%>" >
  </div>
  <div class="form-group">
    <lable for="country">Country</lable>
    <input class="form-control" type="text" readonly id="country" value="<%=doctor.country%>" >
  </div>
  <div class="form-group">
    <lable for="city">City</lable>
    <input class="form-control" type="text" readonly id="city" value="<%=doctor.city%>" >
  </div>
  <div class="form-group">
    <lable for="description">Description</lable>
    <textarea class="form-control" type="text" readonly id="description" value="<%=doctor.description%>" ></textarea>
  </div>
</form>
<h5 class="text-secondary">Clinic Information</h5>
<form>
  <div class="form-group">
    <label for="cl_name">Name</label>
    <input id="cl_name" class="form-control" value="<%=doctor.cl_name%>" type="text" readonly >
  </div>
  <div class="form-group">
    <label for="cl_address">Address</label>
    <input id="cl_address" class="form-control" value="<%=doctor.cl_address%>" type="text" readonly >
  </div>
  <div class="form-group">
    <label for="cl_phone">Phone</label>
    <input id="cl_phone" class="form-control" value="<%=doctor.cl_phone%>" type="text" readonly >
  </div>
  <div class="form-group">
    <label for="cl_fee">Fee</label>
    <input id="cl_fee" class="form-control" value="<%=doctor.cl_fee%>" type="text" readonly >
  </div>
  <div class="form-group">
    <label for="cl_facilities">Facilities</label>
    <textarea id="cl_facilities" class="form-control" type="text" readonly ></textarea>
  </div>
</form>

